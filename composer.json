{
    "name": "murtaza1904/wednesday-theme",
    "description": "Wednesday Theme - A Laravel React admin panel theme with Bootstrap and reusable UI components.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Muhammad Murtaza",
            "email": "contact@muhammadmurtaza.com"
        }
